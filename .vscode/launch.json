    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/test_fr_comia.exe",
            "args": [
                "-image=resources/bald_guys.jpg",
                "-cascade=resources/haarcascade_frontalface_default.xml",
                "-shape_predictor=resources/shape_predictor_68_face_landmarks.dat",
                "-res_net=resources/dlib_face_recognition_resnet_model_v1.dat"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "preLaunchTask": "build"
        }
    ]
}